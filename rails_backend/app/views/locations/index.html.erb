<h1>Locations</h1>

<table class="table">
  <thead>
    <tr>

      <th>id</th>
      <th>city</th>
      <th>street</th>
      <th>house</th>
      <th>room</th>

    </tr>
  </thead>

  <tbody>
    <tr>
      <% @locations.each do |location| %>
        <td><%= location.id %></td>
        <td><%= location.street %></td>
        <td><%= location.house %></td>
        <td><%= location.room %></td>
      <% end %>
    </tbody>
  </tr>
</table>

<br>
